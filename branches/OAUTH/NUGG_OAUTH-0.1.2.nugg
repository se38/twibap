   <parameter CLSNAME="ZCL_OAUTH" CMPNAME="IS_AUTHORIZED" SCONAME="RESULT" VERSION="1" LANGU="E" DESCRIPT="boolsche Variable (X=true, -=false, space=unknown)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="SE38_UWE" CREATEDON="20100819" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="BOOLEAN"/>
